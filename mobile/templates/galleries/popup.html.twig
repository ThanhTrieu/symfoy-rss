{% if list is defined %}
    <div class="slide-contents" style="position: relative;padding: 10px;">
        <a id="goToPrevSlide" class="slide-pre" style="z-index: 1"><i class="icon-left-open-big"></i></a>
        <a id="goToNextSlide" class="slide-next" style="z-index: 1"><i class="icon-right-open-big"></i></a>
        <div class="big-slide">
            <div class="clearfix" style="max-width:480px;position: relative;">
                <ul id="image-gallery" class="gallery list-unstyled cS-hidden">
                    {% for key, item in list %}
                        <li data-link="{{ item.link }}" class="item-photo" data-thumb="{{ item.url }}">
                            <img src="{{ item.url_large }}" alt="{{ item.title }} "/>
                            {#<img src="/images/slide.png"/>#}
                        </li>
                    {% endfor %}
                </ul>
                <p class="count">
                    <span id="current">1</span>
                    of
                    <span id="total"></span>
                </p>
                <h3 class="h3-slide"><span class="text-post"></span> <a rel="nofollow" class="view-more-post" href="" title="View More">(View More)</a></h3>
            </div>
        </div>
    </div>
{% endif %}